# 编译时环境配置 - 使用内存数据库
APP_DATABASE__URL=sqlite::memory:

# JWT配置
APP_JWT__SECRET=test-jwt-secret-key-for-compilation-check
APP_JWT__EXPIRATION=3600
APP_JWT__REFRESH_EXPIRATION=86400
APP_JWT__ISSUER=test-issuer
APP_JWT__AUDIENCE=test-audience

# 安全配置
APP_SECURITY__MAX_LOGIN_ATTEMPTS=5
APP_SECURITY__ACCOUNT_LOCK_DURATION=1800
APP_SECURITY__PASSWORD_MIN_LENGTH=6
APP_SECURITY__BCRYPT_COST=4

# 应用配置
APP_APP__NAME=Test API
APP_APP__VERSION=1.0.0-test
APP_APP__ENVIRONMENT=test
APP_APP__LOG_LEVEL=info
APP_APP__DEBUG=false
APP_APP__TIMEZONE=UTC